  <script>
  $(function(){
          $("#joinform").validate({
              //validation이 끝난 이후의 submit 직전 추가 작업할 부분
              submitHandler: function() {
                  var f = confirm("작성을 완료하겠습니까?");
                  if(f){
                      return true;
                  } else {
                      return false;
                  }
              },
              //규칙
              rules: {
                  memberId: {
                      required : true,
                      email : true
                  },
                  memberPasswd: {
                      required : true,
                      rangelength: [8, 16]
                  },
                  memberPasswd2: {
                      required : true,
                      rangelength: [8, 16],
                      equalTo: '#memberPasswd'
                  },
                  memberName: {
                      required : true
                  },
                  memberBirth: {
                      required : true,
                      digits : true
                  },
                  agree : {
                      required : true
                  }
              },
              //규칙체크 실패시 출력될 메시지
              messages : {
                  memberId: {
                      required : "아이디를 입력하세요",
                      email: "올바른 이메일 주소가 아닙니다."
                  },
                  memberPasswd: {
                      required : "비밀번호를 입력하세요",
                      rangelength: $.validator.format("패스워드 최소 {0}글자 이상 {1}글자 이하로 입력하세요.")
                  },
                  memberPasswd2: {
                      required : "비밀번호를 한번 더 입력하세요",
                      rangelength: $.validator.format("패스워드 확인은 최소 {0}글자 이상 {1}글자 이하로 입력하세요."),
                      equalTo: "패스워드 항목과 일치하지 않습니다."
                  },
                  memberName: {
                      required : "닉네임을 입력하세요"
                  },
                  memberBirth: {
                      required : "생년월일을 입력하세요",
                      digits : "숫자로만 입력해주세요."
                  },
                  agree: {
                      required : "이용약관 동의는 필수입니다."
                  }
              },
              errorPlacement: function(error, element) {
                  if(element.attr("type") == 'checkbox'){
                    element.parent().before(error);
                  }else{
                    element.after(error).after('<br/>');
                    error.css("margin", "0 0 0 5px");
                  }
              }
          });
  })
  </script>
</head>

