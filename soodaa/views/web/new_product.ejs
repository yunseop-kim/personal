<% layout('layout') -%>
<!-- Main Page -->
    <div class="container-fluid noPadding" style="background-color:#e8e8e8;">
        <div class="container textstyle">
            <div class="row text-left latestProjects" style="background-color:#fff;">

                <form action="/web/new_product" method="post">
                    <div class="col-md-12 col-sm-6" style="display:inline-block;text-align:left; width:100%;">
                        <ul>
                            <li style="margin:1em auto; list-style:none;">
                                <strong style="font-size:1.5em">새로운 제품 등록하기</strong>
                            </li>

                            <li style="margin:1em auto; list-style:none;">
                                <select class="form-control" name="productCategory1"
                                        style="width:100%; height:3em; padding:0.5em;">
                                    <option selected>가구/매트/안전용품</option>
                                    <option>건강</option>
                                    <option>기저귀</option>
                                    <option>목욕/위생/스킨케어</option>
                                    <option>물티슈</option>
                                    <option>분유/이유식</option>
                                    <option>수유/이유용품</option>
                                    <option>완구/교구</option>
                                    <option>유모차/외출용품</option>
                                    <option>유아동식품</option>
                                    <option>임신/출산준비물</option>
                                    <option>패션의류잡화</option>
                                </select>
                            </li>


                            <li style="width:100%; margin:1em auto; list-style:none;">
                                <input type="text" name="productBrand"
                                       placeholder="새로운 제품의 브랜드는 무엇인가요?"
                                       style="width:100%; min-width:300px; height:3em; padding:0.5em;">
                            </li>

                            <li style="width:100%; margin:1em auto; list-style:none;">
                                <input type="text" name="productName" placeholder="제품의 이름은 무엇인가요? ex) 테이크 7 디럭스 유모차 블랙"
                                       style="width:100%; min-width:300px; height:3em; padding:0.5em;">
                            </li>

                        </ul>
                    </div>
                    <div class="col-md-12 col-sm-6" style="margin:1em auto; text-align:center;">
                        <button type='submit' class="s_btn"
                                style="margin:1em auto;; margin-right:2em; width:250px; height:40px;">
                            등록하고 리뷰쓰기
                        </button>
                    </div>
                </form>

            </div>

        </div>


        <!-- Footer -->

        <div class="container footer">
            <div class="col-xs-12 col-md-8 noPadding">
                <p id="footerLogoText">Soodaa © 2015 | <a href="/privacy">개인정보취급방침</a> | <a href="/ask">운영자에게 문의하기</a></p>
            </div>
            <div class="col-xs-12 col-md-4 noPadding">
                <div class="socialButtons responsiveAlign">
                    <a href="#" class="noPadding">
                        <img src="/static/image/twitter.svg" alt="image1"
                             onclick="alert('준비중입니다');">
                    </a>
                    <a href="#" class="noPadding">
                        <img src="/static/image/facebook.svg" alt="image1"
                             onclick="alert('준비중입니다');">
                    </a>
                    <a href="#" class="noPadding">
                        <img src="/static/image/behance.svg" alt="image1"
                             onclick="alert('준비중입니다');">
                    </a>
                </div>
            </div>
        </div>

    </div>



<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<script src="/static/js/bootstrap.min.js"></script>
<script src="/static/js/myscript.js"></script>
